<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<fx:root maxHeight="1.7976931348623157E308" spacing="35.0" style="-fx-background-color: -fx-base;" type="VBox" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <HBox alignment="CENTER" spacing="15.0">
         <children>
            <Label text="Enter target level:" />
            <Spinner fx:id="levelSpinner" editable="true" prefHeight="25.0" prefWidth="60.0" />
            <Button fx:id="addTrackerButton" mnemonicParsing="false" style="-fx-background-color: -fx-background;" text="Add tracker" />
         </children>
      </HBox>
   </children>
</fx:root>
