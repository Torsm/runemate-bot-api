<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root spacing="12.0" type="VBox" HBox.hgrow="ALWAYS" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <FlowPane fx:id="dataFlowPane" hgap="20.0" prefWrapLength="0.0" style="-fx-background-color: -fx-base;" vgap="30.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <VBox fx:id="skillVBox">
               <children>
                  <Label fx:id="nameLabel" styleClass="accented-text" />
                  <Label styleClass="vanity-title" text="Skill" />
                  <Label fx:id="typeLabel" styleClass="vanity-text" />
               </children>
            </VBox>
            <HBox fx:id="dataHBox" spacing="20.0">
               <children>
                  <VBox fx:id="levelVBox">
                     <children>
                        <Label fx:id="levelLabel" styleClass="accented-text" />
                        <Label styleClass="vanity-title" text="Level" />
                        <Label fx:id="levelsGainedLabel" styleClass="vanity-text" />
                     </children>
                  </VBox>
                  <VBox fx:id="experienceVBox">
                     <children>
                        <Label fx:id="experienceLabel" styleClass="accented-text" />
                        <Label styleClass="vanity-title" text="Experience" />
                        <Label fx:id="experienceRateLabel" styleClass="vanity-text" />
                     </children>
                  </VBox>
                  <VBox fx:id="actionsVBox">
                     <children>
                        <Label fx:id="actionsLabel" styleClass="accented-text" />
                        <Label styleClass="vanity-title" text="Actions" />
                        <Label fx:id="actionsRateLabel" styleClass="vanity-text" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </FlowPane>
   </children>
</fx:root>
